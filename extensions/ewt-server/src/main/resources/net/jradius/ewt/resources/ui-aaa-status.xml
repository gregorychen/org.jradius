<!DOCTYPE CAP-UI SYSTEM "/etc/coova/cap-ui.dtd">
<cap-ui>
  <menu name="Status">
    <menu name="Configuration">
      <inputset name="Main Configuration">
	<input label="" type="file" dir="/usr/local/etc/coova" file="config.xml"/>
      </inputset>
    </menu>
    <menu name="Files">
      <inputset name="GUI Files">
	<input label="File" type="filelist" def="Select File" dir="/usr/local/etc/coova">
	  <option name="Server" value="ui-aaa-server.xml"/>
	  <option name="Proxy" value="ui-aaa-proxy.xml"/>
	  <option name="Modules" value="ui-aaa-modules.xml"/>
	  <option name="Users" value="ui-aaa-users.xml"/>
	  <option name="XSLT" value="xml2radiusd.xsl"/>
	</input>
      </inputset>
   </menu>
 </menu>
</cap-ui>