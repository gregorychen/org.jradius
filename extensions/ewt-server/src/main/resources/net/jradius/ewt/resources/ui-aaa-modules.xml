<!DOCTYPE CAP-UI SYSTEM "/etc/coova/cap-ui.dtd">
<cap-ui>
  <menu name="Modules">
    <menu name="Basic">
      <inputset name="PAP Module" param="rlm_pap" object="radiusd.module">
	<input label="Enabled" type="check" value="yes" key="rlm_pap">
	  <dependency value="yes" change="pap" />
	</input>
        <inputset param="pap" object="radiusd.module.pap">
          <input label="Encryption" key="encryption_scheme">
	    <option name="Auto" value="auto"/>
	    <option name="Clear" value="clear"/>
	    <option name="Crypt" value="crypt"/>
	    <option name="MD5" value="md5"/>
	    <option name="SHA1" value="sha1"/>
	    <option name="MS-NT" value="nt"/>
	    <option name="MS-LM" value="lm"/>
	    <option name="SMD5" value="smd5"/>
	    <option name="SSHA" value="ssha"/>
          </input>
          <input label="Auto Header" key="auto_header" type="check" value="yes" />
        </inputset>
        <help label="About the module" id="aaaModulePAP"/>
      </inputset>
      <inputset name="CHAP Module" param="rlm_chap" object="radiusd.module">
	<input label="Enabled" type="check" value="yes" key="rlm_chap">
	</input>
        <help label="About the module" id="aaaModuleCHAP"/>
      </inputset>
      <inputset name="MSCHAP Module" param="rlm_mschap" object="radiusd.module">
	<input label="Enabled" type="check" value="yes" key="rlm_mschap">
	</input>
        <help label="About the module" id="aaaModuleMSCHAP"/>
      </inputset>
      <inputset name="PAM Module" param="rlm_pam" object="radiusd.module">
	<input label="Enabled" type="check" value="yes" key="rlm_pam">
	</input>
        <help label="About the module" id="aaaModulePAM"/>
      </inputset>
      <inputset name="UNIX Module" param="rlm_unix" object="radiusd.module">
	<input label="Enabled" type="check" value="yes" key="rlm_unix">
	</input>
        <help label="About the module" id="aaaModuleUNIX"/>
      </inputset>
      <inputset name="Digest Module" param="rlm_digest" object="radiusd.module">
	<input label="Enabled" type="check" value="yes" key="rlm_digest">
	</input>
        <help label="About the module" id="aaaModuleDigest"/>
      </inputset>
    </menu>
    <menu name="EAP">
      <inputset name="EAP Module" param="rlm_eap" object="radiusd.module">
	<input label="Enabled" type="check" value="yes" key="rlm_eap">
	  <dependency value="yes" change="eap" />
	</input>
        <help label="About the module" id="aaaModuleEAP"/>
      </inputset>
      <inputset name="EAP Protocols" object="radiusd.module" param="eap">
	<input label="MD5" type="check" value="yes" key="rlm_eap_md5">
	  <option name="Enabled" value="yes"/>
	</input>
	<input label="MSCHAPv2" type="check" value="yes" key="rlm_eap_mschapv2">
	  <option name="Enabled" value="yes"/>
	</input>
        <input label="TLS" type="check" value="yes" key="rlm_eap_tls">
	  <option name="Enabled" value="yes"/>
	  <dependency value="yes" change="rlmttls"/>
	  <dependency value="yes" change="rlmpeap"/>
	  <dependency value="yes" change="tls"/>
	</input>
	<input label="TTLS" type="check" value="yes" param="rlmttls" key="rlm_eap_ttls">
	  <option name="Enabled" value="yes"/>
	  <dependency value="yes" change="ttls" />
	</input>
	<input label="PEAP" type="check" value="yes" param="rlmpeap" key="rlm_eap_peap">
	  <option name="Enabled" value="yes"/>
	  <dependency value="yes" change="peap" />
	</input>
	<input label="LEAP" type="check" value="yes" key="rlm_eap_leap">
	  <option name="Enabled" value="yes"/>
	</input>
        <help label="About EAP protocols" id="aaaModuleEAPProtocols"/>
      </inputset>
      <inputset name="TLS Settings" object="radiusd.module.tls" param="tls">
	<input label="CA File" key="CA_file"/>
	<input label="Certificate File" key="certificate_file"/>
	<input label="Private Key File" key="private_key_file"/>
	<input label="Private Key Password" key="private_key_password"/>
	<input label="DH File" key="dh_file"/>
	<input label="Random File" key="random_file"/>
	<input label="Fragment Size" key="fragment_size" dtype="int"/>
	<input label="Include Length" key="include_length" type="check"	value="yes"/>
        <help label="About EAP-TLS" id="aaaModuleEAPTLS"/>
      </inputset>
      <inputset name="TTLS Settings" object="radiusd.module.ttls" param="ttls">
	<input label="Default EAP Type" key="default_eap_type">
	  <option name="MSCHAPv2" value="mschapv2"/>
	  <option name="TLS" value="tls"/>
	  <option name="TTLS" value="ttls"/>
	  <option name="PEAP" value="peap"/>
	  <option name="MD5" value="md5"/>
	</input>
	<input label="Proxy Tunnel as EAP" key="proxy_tunneled_request_as_eap" type="check" value="yes"/>
	<input label="Copy Request to Tunnel" key="copy_request_to_tunnel" type="check" value="yes"/>
	<input label="Use Tunnel Reply" key="use_tunneled_reply" type="check" value="yes"/>
        <help label="About EAP-TTLS" id="aaaModuleEAPTTLS"/>
      </inputset>
      <inputset name="PEAP Settings" object="radiusd.module.peap" param="peap">
	<input label="Default EAP Type" key="default_eap_type">
	  <option name="MSCHAPv2" value="mschapv2"/>
	  <option name="TLS" value="tls"/>
	  <option name="TTLS" value="ttls"/>
	  <option name="PEAP" value="peap"/>
	  <option name="MD5" value="md5"/>
	</input>
	<input label="Proxy Tunnel as EAP" key="proxy_tunneled_request_as_eap" type="check" value="yes"/>
	<input label="Copy Request to Tunnel" key="copy_request_to_tunnel" type="check" value="yes"/>
	<input label="Use Tunnel Reply" key="use_tunneled_reply" type="check" value="yes"/>
        <help label="About PEAP" id="aaaModulePEAP"/>
      </inputset>
    </menu>
    <menu name="Realm">
      <inputset name="Realm Module" param="rlm_realm" object="radiusd.module">
	<input label="Enabled" type="check" value="yes" key="rlm_realm">
	  <dependency value="yes" change="realm" />
	</input>
        <help label="About the module" id="aaaModuleRealm"/>
      </inputset>
      <inputset name="Realm Format" object="radiusd.module.realm" param="realm" singleton="false">
	<input label="Name" key="id"/>
	<input label="Format" key="format">
	  <option name="Suffix" value="suffix"/>
	  <option name="Prefix" value="prefix"/>
	</input>
	<input label="Delimiter" key="delimiter"/>
        <inputset singleton="true">
	  <input label="Ignore Default" type="check" key="ignore_default">
	    <option name="Enabled" value="yes"/>
	  </input>
	  <input label="Ignore Null" type="check" key="ignore_null">
	    <option name="Enabled" value="yes"/>
	  </input>
        </inputset>
        <help label="About realm formats" id="aaaRealmFormat"/>
      </inputset>
    </menu>
    <menu name="SQL">
      <inputset name="SQL Module" param="rlm_sql" object="radiusd.module">
	<input label="Enabled" type="check" value="yes" key="rlm_sql">
	  <dependency value="yes" change="sql" />
	</input>
        <help label="About the module" id="aaaModuleSQL"/>
      </inputset>
    </menu>    
    <menu name="Rewrite">
      <inputset name="Attribute Rewrite Module" param="rlm_rewrite" object="radiusd.module">
	<input label="Enabled" type="check" value="yes" key="rlm_rewrite">
	  <dependency value="yes" change="rewrite" />
	</input>
        <help label="About the module" id="aaaModuleRewrite"/>
      </inputset>
      <inputset name="Rewrite Rules" param="rewrite" object="radiusd.module.rewrite" singleton="false">
	<input label="Name" key="id" required="true" dtype="word"></input>
	<input label="Attribute" key="attribute"></input>
        <inputset singleton="true">
	  <input label="" key="searchin"></input>
	  <input label="" key="searchfor"></input>
	  <input label="" key="replacewith"></input>
	  <input label="" key="maxmatches"></input>
	  <input label="" key="new_attribute"></input>
        </inputset>
        <help label="About rewrite rules" id="aaaModuleRewriteRules"/>
      </inputset>
    </menu>    
    <menu name="Perl">
      <inputset name="Perl Module" param="rlm_perl" object="radiusd.module">
	<input label="Enabled" type="check" value="yes" key="rlm_perl">
	  <dependency value="yes" change="perl" />
	</input>
        <help label="About the module" id="aaaModulePerl"/>
      </inputset>
      <inputset name="Perl Settings" param="perl" object="radiusd.module.perl">
	<input label="Module" key="module"></input>
	<input label="Authorize Function" key="func_authorize"></input>
	<input label="Authenticate Function" key="func_authenticate"></input>
	<input label="Post Auth Function" key="func_post_auth"></input>
	<input label="Pre Acct Function" key="func_preacct"></input>
	<input label="Acct Function" key="func_accounting"></input>
	<input label="Pre Proxy Function" key="func_pre_proxy"></input>
	<input label="Post Proxy Function" key="func_post_proxy"></input>
	<input label="Start Acct Function" key="func_start_accounting"></input>
	<input label="Stop Acct Function" key="func_stop_accounting"></input>
	<input label="Check Simul Function" key="func_checksimul"></input>
	<input label="Detach Function" key="func_detach"></input>
	<input label="XLAT Function" key="func_xlat"></input>
	<input label="Perl Flags" key="perl_flags"></input>
        <help label="About perl settings" id="aaaPerlSettings"/>
      </inputset>
    </menu>    
    <menu name="LDAP">
      <inputset name="LDAP Module" param="rlm_ldap" object="radiusd.module">
	<input label="Enabled" type="check" value="yes" key="rlm_ldaps">
	  <dependency value="yes" change="ldap" />
	</input>
        <help label="About the module" id="aaaModuleLDAP"/>
      </inputset>
      <inputset name="LDAP Settings" param="ldap" object="radiusd.module.ldap">
	<input label="Server" key="server"></input>
	<input label="Port" key="port"></input>
	<input label="Password" key="password"></input>
	<input label="Identity" key="identity"></input>
	<input label="Net Timeout" key="net_timeout"></input>
	<input label="Timeout" key="timeout"></input>
	<input label="Time Limit" key="timelimit"></input>
	<input label="" key="tls_mode"></input>
	<input label="" key="start_tls"></input>
	<input label="" key="tls_cacertfile"></input>
	<input label="" key="tls_cacertdir"></input>
	<input label="" key="tls_certfile"></input>
	<input label="" key="tls_keyfile"></input>
	<input label="" key="tls_randfile"></input>
	<input label="" key="tls_require_cert"></input>
	<input label="" key="basedn"></input>
	<input label="" key="filter"></input>
	<input label="" key="base_filter"></input>
	<input label="" key="default_profile"></input>
	<input label="" key="profile_attribute"></input>
	<input label="" key="password_header"></input>
	<input label="" key="password_attribute"></input>
	<input label="" key="auto_header"></input>
	<input label="" key="access_attr"></input>
	<input label="" key="access_attr_used_for_allow"></input>
	<input label="" key="groupname_attribute"></input>
	<input label="" key="groupmembership_filter"></input>
	<input label="" key="groupmembership_attribute"></input>
	<input label="" key="dictionary_mapping"></input>
	<input label="" key="ldap_debug"></input>
	<input label="" key="ldap_connections_number"></input>
	<input label="" key="compare_check_items"></input>
	<input label="" key="do_xlat"></input>
        <help label="About LDAP settings" id="aaaLDAPSettings"/>
      </inputset>
    </menu>    
    <menu name="JRadius">
      <inputset name="JRadius Module" param="rlm_jradius" object="radiusd.module">
	<input label="Enabled" type="check" value="yes" key="rlm_jradius">
	  <dependency value="yes" change="jradius" />
	</input>
        <help label="About the module" id="aaaModuleJRadius"/>
      </inputset>
      <inputset name="JRadius Settings" param="jradius" object="radiusd.module.jradius">
	<input label="Application Name" key="name"></input>
	<input label="Primary Server" key="primary"></input>
	<input label="Secondary Server" key="secondary"></input>
	<input label="Tertiary Server" key="tertiary"></input>
	<input label="On Fail" key="onfail">
	  <option name="NOOP" value="NOOP"/>
	  <option name="FAIL" value="FAIL"/>
	</input>
	<input label="Keep Alive" type="check" value="on" key="keepalive">
	  <dependency value="on" change="jradius:connections" />
	</input>
	<input label="Connections" param="jradius:connections" key="connections"/>
	<input label="Allow Code Change" type="check" value="yes" key="allow_codechange">
	</input>
	<input label="Allow ID Change" type="check" value="yes" key="allow_idchange">
	</input>
        <help label="About JRadius settings" id="aaaJRadiusSettings"/>
      </inputset>
    </menu>    
    <menu name="More..">
      <inputset name="Python Module" param="rlm_python" object="radiusd.module">
	<input label="Enabled" type="check" value="yes" key="rlm_python">
	</input>
        <help label="About the module" id="aaaModulePython"/>
      </inputset>
    </menu>    
  </menu>
</cap-ui>
