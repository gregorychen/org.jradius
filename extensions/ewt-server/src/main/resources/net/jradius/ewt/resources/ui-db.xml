<!DOCTYPE CAP-UI SYSTEM "/etc/coova/cap-ui.dtd">
<cap-ui>
	<menu name="Accounting">
		<menu name="Sessions">
			<inputset name="Session Accounting" service="table" table="radacct" object="radacct" readOnly="true" singleton="false">
				<input label="SessionID" key="acctsessionid"></input>
				<input label="Username" key="username"></input>
				<input label="Groupname" key="groupname"></input>
				<input label="Realm" key="realm"></input>
				<input label="Start Time" key="acctstarttime"></input>
				<input label="Stop Time" key="acctstoptime"></input>
				<input label="Duration" key="acctsessiontime" type="int"></input>
				<inputset singleton="true">
					<input label="NAS-IP-Address" key="nasipaddress"></input>
					<input label="NAS-Port-Id" key="nasportid"></input>
					<input label="NAS-Port-Type" key="nasporttype"></input>
					<input label="Acct-Authentic" key="acctauthentic"></input>
					<input label="Acct-Input-Octets" key="acctinputoctets" type="int"></input>
					<input label="Acct-Output-Octets" key="acctoutputoctets" type="int"></input>
					<input label="Called-Station-Id" key="calledstationid"></input>
					<input label="Calling-Station-Id" key="callingstationid"></input>
					<input label="Acct-Terminate-Cause" key="acctterminatecause"></input>
					<input label="Service-Type" key="servicetype"></input>
					<input label="Framed-Protocol" key="framedprotocol"></input>
					<input label="Framed-IP-Address" key="framedipaddress"></input>
					<input label="Acct-Start-Delay" key="acctstartdelay" type="int"></input>
					<input label="Acct-Stop-Delay" key="acctstopdelay" type="int"></input>
				</inputset>
			</inputset>
		</menu>
	</menu>
	<menu name="Devices">
		<menu name="NAS">
			<inputset name="NAS" object="nas" popup="true" service="table" table="nas" singleton="false">
		        <input label="Name" key="nasname" required="true"></input>
		        <input label="Shortname" key="shortname" required="true"></input>
		        <input label="Type" key="type"></input>
		        <input label="Ports" key="ports"></input>
		        <input label="Secret" key="secret" required="true"></input>
				<inputset name="Details" singleton="true">
			        <input label="Community" key="community"></input>
			        <input label="Description" key="description"></input>
					<input label="Latitude" key="latitude" />
					<input label="Longitude" key="longitude" />
				</inputset>
				<inputset name="Location Map" object="nas" singleton="true">
					<input type="map" width="300px" height="250px" />
				</inputset>
				<help label="About the NAS table" id="nas"/>
			</inputset>
		</menu>
	</menu>
	<menu name="Check">
		<menu name="Check">
			<inputset name="Request Check" object="radcheck" service="table" table="radcheck" singleton="false">
		        <input label="Username" key="username" required="true"></input>
		        <input label="Attribute" key="attribute" type="suggest" service="attributes" required="true"></input>
		        <input label="Op" key="op">
					<option name=":= (set)" value=":="/>
					<option name="+= (add)" value="+="/>
					<option name="-= (subtract)" value="-="/>
					<option name="= (equal)" value="="/>
					<option name="!= (not equal)" value="!="/>
					<option name=">= (greater or equal)" value=">="/>
					<option name="> (greater)" value=">"/>
					<option name="<= (lessor or equal)" value="<="/>
					<option name="< (lessor)" value="<"/>
					<option name="=~ (expression equal)" value="=~"/>
					<option name="!~ (expression not equal)" value="!~"/>
					<option name="=* (comparison is true)" value="=*"/>
					<option name="!* (comparison is false)" value="!*"/>
					<option name="== (comparison equal)" value="=="/>
		        </input>
		        <input label="Value" key="value"></input>
				<help label="About the Check table" id="radcheck"/>
			</inputset>
		</menu>
		<menu name="GroupCheck">
			<inputset name="Request Group Check" object="radgroupcheck" service="table" table="radgroupcheck" singleton="false">
		        <input label="Groupname" key="groupname" required="true"></input>
		        <input label="Attribute" key="attribute" type="suggest" service="attributes" required="true"></input>
		        <input label="Op" key="op">
					<option name=":= (set)" value=":="/>
					<option name="+= (add)" value="+="/>
					<option name="-= (subtract)" value="-="/>
					<option name="= (equal)" value="="/>
					<option name="!= (not equal)" value="!="/>
					<option name=">= (greater or equal)" value=">="/>
					<option name="> (greater)" value=">"/>
					<option name="<= (lessor or equal)" value="<="/>
					<option name="< (lessor)" value="<"/>
					<option name="=~ (expression equal)" value="=~"/>
					<option name="!~ (expression not equal)" value="!~"/>
					<option name="=* (comparison is true)" value="=*"/>
					<option name="!* (comparison is false)" value="!*"/>
					<option name="== (comparison equal)" value="=="/>
		        </input>
		        <input label="Value" key="value"></input>
				<help label="About the GroupCheck table" id="radgroupcheck"/>
			</inputset>
		</menu>
	</menu>
	<menu name="Reply">
		<menu name="Reply">
			<inputset name="Reply Attributes" service="table" table="radreply" object="radreply" singleton="false">
				<input label="Username" key="username" required="true"></input>
			    <input label="Attribute" key="attribute" type="suggest" service="attributes" required="true"></input>
				<input label="Op" key="op">
					<option name=":= (set)" value=":="/>
					<option name="+= (add)" value="+="/>
					<option name="-= (subtract)" value="-="/>
					<option name="= (equal)" value="="/>
					<option name="!= (not equal)" value="!="/>
					<option name=">= (greater or equal)" value=">="/>
					<option name="> (greater)" value=">"/>
					<option name="<= (lessor or equal)" value="<="/>
					<option name="< (lessor)" value="<"/>
					<option name="=~ (expression equal)" value="=~"/>
					<option name="!~ (expression not equal)" value="!~"/>
					<option name="=* (comparison is true)" value="=*"/>
					<option name="!* (comparison is false)" value="!*"/>
					<option name="== (comparison equal)" value="=="/>
				</input>
				<input label="Value" key="value"></input>
				<help label="About the Reply table" id="radreply"/>
			</inputset>
		</menu>
		<menu name="GroupReply">
			<inputset name="Group Reply Attributes" service="table" table="radgroupreply" object="radgroupreply" singleton="false">
				<input label="Groupname" key="groupname"></input>
		        <input label="Attribute" key="attribute" type="suggest" service="attributes"></input>
				<input label="Op" key="op">
					<option name=":= (set)" value=":="/>
					<option name="+= (add)" value="+="/>
					<option name="-= (subtract)" value="-="/>
					<option name="= (equal)" value="="/>
					<option name="!= (not equal)" value="!="/>
					<option name=">= (greater or equal)" value=">="/>
					<option name="> (greater)" value=">"/>
					<option name="<= (lessor or equal)" value="<="/>
					<option name="< (lessor)" value="<"/>
					<option name="=~ (expression equal)" value="=~"/>
					<option name="!~ (expression not equal)" value="!~"/>
					<option name="=* (comparison is true)" value="=*"/>
					<option name="!* (comparison is false)" value="!*"/>
					<option name="== (comparison equal)" value="=="/>
				</input>
				<input label="Value" key="value"></input>
				<help label="About the GroupReply table" id="radgroupreply"/>
			</inputset>
		</menu>
	</menu>
	<menu name="Other">
		<menu name="UserGroups">
			<inputset name="User Groups" service="table" table="radusergroup" object="radusergroup" singleton="false">
				<input label="Username" key="username" required="true"></input>
				<input label="Groupname" key="groupname" required="true"></input>
				<input label="Priority" key="priority" type="int"></input>
				<help label="About the UserGroup table" id="radreply"/>
			</inputset>
		</menu>
		<menu name="PostAuth">
			<inputset name="Post Authentication Attributes" service="table" table="radpostauth" object="radpostauth" singleton="false">
				<input label="Username" key="username" required="true"></input>
				<input label="Password" key="pass"></input>
				<input label="Reply" key="reply"></input>
				<input label="Auth Date" key="authdate" type="date"></input>
				<help label="About the PostAuth table" id="radpostauth"/>
			</inputset>
		</menu>
		<menu name="IPPools">
			<inputset name="DHCP IP Pools" service="table" table="radippool" object="radippool" singleton="false">
				<input label="Name" key="pool_name" required="true"></input>
				<input label="Framed-IP-Address" key="framedipaddress"></input>
				<input label="NAS-IP-Address" key="nasipaddress"></input>
				<input label="Called-Station-Id" key="calledstationid"></input>
				<input label="Calling-Station-Id" key="callingstationid"></input>
				<input label="Expiry Time" key="expiry_time"></input>
				<input label="Username" key="username"></input>
				<input label="Pool Key" key="pool_key"></input>
				<help label="About the IPPools table" id="radippool"/>
			</inputset>
		</menu>
		<menu name="UI Help">
			<inputset name="uihelp" service="table" table="uihelp" object="uihelp" singleton="false">
				<input label="Key" key="keyid" required="true"></input>
				<input label="Help Text" type="textarea" key="helptext"></input>
			</inputset>
		</menu>
	</menu>
</cap-ui>
